<!DOCTYPE html>
<html>
    <title> Investments </title>
    <body>
        <a href="/">Back</a>
        <h1>Current Applications</h1>

<table class="table table-striped table-primary">
        <thead>
          <tr>
            <th>First Name</th>
            <th>Surname</th>
            <th>Date of Birth</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Account Type</th>
            <th>Amount</th>
          </tr>
        </thead>
        <tbody>
          <%if (entries.length >0){ %>
          <% entries.forEach(function(row){ %>
          <tr>
            <td><%= row.first_name %></td>
            <td><%= row.surname %></td>
            <td><%= row.dob %></td>
            <td><%= row.email %></td>
            <td><%= row.num %></td>
            <td><%= row.opt %></td>
            <td><%= row.amount %></td>
          </tr>
          <% })}else{ %>
            <tr>
                <td colspan="6">No Record Found</td>
                
              </tr>
            <% } %>
        </tbody>
      </table>
    </body>
</html>